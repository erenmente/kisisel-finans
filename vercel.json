{
    "builds": [
        {
            "src": "src/web_app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/web_app.py"
        }
    ],
    "build": {
        "env": {
            "ENABLE_VC_BUILD": "1"
        }
    }
}